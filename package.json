{
  "name": "sling-framework",
  "description": "Sling Web Framework main repo.",
  "version": "1.12.0",
  "private": true,
  "flat": true,
  "scripts": {
    "version-bump": "lerna publish --skip-git --skip-npm",
    "prepublish-packages": "npm install",
    "publish-packages": "lerna exec --bail=false -- npm publish",
    "semantic-release": "lerna exec --concurrency 1 -- npx --no-install semantic-release -e semantic-release-monorepo",
    "lint": "./node_modules/.bin/eslint ./packages/**/*.js",
    "send-coverage": "cat ./coverage/integration/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postinstall": "lerna bootstrap",
    "build": "node ./scripts/build.js",
    "build-prod": "CI_ENV=true node ./scripts/build.js",
    "start": "node ./scripts/start.js",
    "nightwatch": "node ./scripts/night.js",
    "pretest": "npm run lint",
    "test": "node ./scripts/test.js",
    "test-debug": "./node_modules/.bin/cross-env DEBUG=true node ./scripts/test.js",
    "prespec": "npm run lint",
    "spec": "node ./scripts/spec.js",
    "server": "node ./scripts/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "camelcase": "^5.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "fs-extra": "^5.0.0",
    "globby": "^8.0.1",
    "html-replace-webpack-plugin": "^2.5.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.0.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lerna": "^3.4.1",
    "mocha": "^5.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^13.1.0",
    "opn": "^5.3.0",
    "postcss": "^6.0.21",
    "postcss-import": "^11.1.0",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.9.17",
    "semantic-release-monorepo": "^6.1.1",
    "semistandard": "^12.0.1",
    "shelljs": "^0.8.2",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "string-replace-async": "^1.2.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/npm": "^5.0.4",
    "nightwatch": "^1.0.11",
    "selenium-download": "^2.0.13",
    "puppeteer": "^1.9.0"
  },
  "release": {
    "branch": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stone-payments/sling-framework.git"
  },
  "author": "Stone Pagamentos",
  "bugs": {
    "url": "https://github.com/stone-payments/sling-framework/issues"
  },
  "homepage": "https://github.com/stone-payments/sling-framework#readme",
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}
